{
  "manifest_version": 3,
  "name": "N플리마켓 템플릿 도구",
  "version": "1.0",
  "description": "네이버 플리마켓 상품 등록 자동화 도구",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://fleamarket.naver.com/*"
      ],
      "js": [
        "scripts/constants.js",
        "scripts/utils.js",
        "scripts/extractors.js",
        "scripts/setters.js",
        "content.js"
      ]
    }
  ]
}